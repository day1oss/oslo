{
	"name": "@day1co/oslo",
	"type": "module",
	"version": "1.2.0",
	"description": "A collection of auth-related utilities",
	"main": "./dist/cjs/index.js",
	"types": "./dist/types/index.d.ts",
	"module": "./dist/esm/index.js",
	"scripts": {
		"build": "rm -rf dist/* && tsc --project tsconfig.build.esm.json && tsc --project tsconfig.build.cjs.json && tsc --project tsconfig.build.dts.json",
		"format": "prettier -w .",
		"lint": "eslint src",
		"test": "vitest run --sequence.concurrent"
	},
	"files": [
		"/dist/"
	],
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"default": "./dist/cjs/index.js"
		},
		"./cookie": {
			"types": "./dist/types/cookie/index.d.ts",
			"require": "./dist/cjs/cookie/index.js",
			"import": "./dist/esm/cookie/index.js",
			"default": "./dist/cjs/cookie/index.js"
		},
		"./crypto": {
			"types": "./dist/types/crypto/index.d.ts",
			"require": "./dist/cjs/crypto/index.js",
			"import": "./dist/esm/crypto/index.js",
			"default": "./dist/cjs/crypto/index.js"
		},
		"./encoding": {
			"types": "./dist/types/encoding/index.d.ts",
			"require": "./dist/cjs/encoding/index.js",
			"import": "./dist/esm/encoding/index.js",
			"default": "./dist/cjs/encoding/index.js"
		},
		"./jwt": {
			"types": "./dist/types/jwt/index.d.ts",
			"require": "./dist/cjs/jwt/index.js",
			"import": "./dist/esm/jwt/index.js",
			"default": "./dist/cjs/jwt/index.js"
		},
		"./oauth2": {
			"types": "./dist/types/oauth2/index.d.ts",
			"require": "./dist/cjs/oauth2/index.js",
			"import": "./dist/esm/oauth2/index.js",
			"default": "./dist/cjs/oauth2/index.js"
		},
		"./otp": {
			"types": "./dist/types/otp/index.d.ts",
			"require": "./dist/cjs/otp/index.js",
			"import": "./dist/esm/otp/index.js",
			"default": "./dist/cjs/otp/index.js"
		},
		"./password": {
			"types": "./dist/types/password/index.d.ts",
			"require": "./dist/cjs/password/index.js",
			"import": "./dist/esm/password/index.js",
			"default": "./dist/cjs/password/index.js"
		},
		"./request": {
			"types": "./dist/types/request/index.d.ts",
			"require": "./dist/cjs/request/index.js",
			"import": "./dist/esm/request/index.js",
			"default": "./dist/cjs/request/index.js"
		},
		"./webauthn": {
			"types": "./dist/types/webauthn/index.d.ts",
			"require": "./dist/cjs/webauthn/index.js",
			"import": "./dist/esm/webauthn/index.js",
			"default": "./dist/cjs/webauthn/index.js"
		}
	},
	"keywords": [
		"auth",
		"oauth2",
		"jwt",
		"crypto",
		"webauthn",
		"otp",
		"encoding",
		"auth",
		"random"
	],
	"author": "pilcrowOnPaper",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/oslo"
	},
	"devDependencies": {
		"@scure/base": "^1.1.5",
		"@types/node": "^20.11.28",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"auri": "1.0.2",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"typescript": "^5.4.2",
		"vitest": "^1.4.0"
	},
	"dependencies": {
		"@node-rs/argon2": "1.8.0",
		"@node-rs/bcrypt": "1.10.1"
	}
}
