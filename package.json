{
	"name": "@day1co/oslo",
	"type": "commonjs",
	"version": "1.2.1",
	"description": "A collection of auth-related utilities",
	"main": "./index.js",
	"types": "./index.d.ts",
	"scripts": {
		"build": "rm -rf dist/* && tsc --project tsconfig.build.cjs.json",
		"publish": "npm run build && cp package.json dist/cjs && cp README.md dist/cjs && cp LICENSE dist/cjs && cd dist/cjs",
		"format": "prettier -w .",
		"lint": "eslint src",
		"test": "vitest run --sequence.concurrent"
	},
	"keywords": [
		"auth",
		"oauth2",
		"jwt",
		"crypto",
		"webauthn",
		"otp",
		"encoding",
		"auth",
		"random"
	],
	"author": "pilcrowOnPaper",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/pilcrowOnPaper/oslo"
	},
	"devDependencies": {
		"@scure/base": "^1.1.5",
		"@types/node": "^20.11.28",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"auri": "1.0.2",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"typescript": "^5.4.2",
		"vitest": "^1.4.0"
	},
	"dependencies": {
		"@node-rs/argon2": "1.8.0",
		"@node-rs/bcrypt": "1.10.1"
	}
}
